{
    "name": "codejet/kaba",
    "description": "Kaba Keyless Entry SDK / API Wrapper",
    "keywords": [
        "codejet",
        "kaba"
    ],
    "homepage": "https://github.com/CodeJetNet/Kaba",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Houghtelin",
            "email": "josh@findsomehelp.com",
            "homepage": "http://codejet.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "CodeJet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeJet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
